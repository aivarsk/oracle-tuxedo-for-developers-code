*RESOURCES
MASTER tuxapp
MODEL SHM
IPCKEY 32789

*MACHINES
"@UNAME@" LMID=tuxapp
    TUXCONFIG="@CURDIR@/tuxconfig"
    TUXDIR="@TUXDIR@"
    APPDIR="@CURDIR@"
    TLOGNAME="TLOG"
    TLOGDEVICE="@CURDIR@/tlog"
    TLOGSIZE=100

*GROUPS
GROUP1 LMID=tuxapp GRPNO=1 TMSNAME=TMS TMSCOUNT=2
DMGROUP LMID=tuxapp GRPNO=2

*SERVERS
"tolower.py" SRVGRP=GROUP1 SRVID=1
    REPLYQ=Y MAXGEN=2 RESTART=Y GRACE=0
    CLOPT="-s TOLOWER:PY"

"DMADM" SRVGRP=DMGROUP SRVID=11 REPLYQ=Y MAXGEN=2 RESTART=Y GRACE=0
"GWADM" SRVGRP=DMGROUP SRVID=12 REPLYQ=Y MAXGEN=2 RESTART=Y GRACE=0
"GWTDOMAIN" SRVGRP=DMGROUP SRVID=13 REPLYQ=Y MAXGEN=2 RESTART=Y GRACE=0
