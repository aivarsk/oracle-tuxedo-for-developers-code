*DM_LOCAL
"LOWER" GWGRP=DMGROUP
        TYPE=TDOMAIN
        ACCESSPOINTID="LOWER"
        DMTLOGDEV="@CURDIR@/dmtlog"
        DMTLOGNAME="DMTLOG"
	DMTLOGSIZE=100

*DM_REMOTE
"UPPER" TYPE=TDOMAIN
        ACCESSPOINTID="UPPER"

*DM_TDOMAIN
"UPPER" NWADDR="//localhost:11000"
        CONNECTION_POLICY=ON_STARTUP
        RETRY_INTERVAL=60
        MAXRETRY=MAXLONG
        TCPKEEPALIVE=YES
        DMKEEPALIVE=10

"LOWER" NWADDR="//localhost:12000"
        CONNECTION_POLICY=ON_STARTUP
        RETRY_INTERVAL=60
        MAXRETRY=MAXLONG
        TCPKEEPALIVE=YES
        DMKEEPALIVE=10

*DM_EXPORT
TOLOWER

*DM_IMPORT
TOUPPER 
