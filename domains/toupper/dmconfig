*DM_LOCAL
"UPPER"  GWGRP=DMGROUP
        TYPE=TDOMAIN
        ACCESSPOINTID="UPPER"
        DMTLOGDEV="/home/oracle/toupper/DMTLOG"
        DMTLOGNAME="DMTLOG"
        CONNECTION_POLICY=ON_STARTUP
        MAXRETRY=MAXLONG
        RETRY_INTERVAL=60
        BLOCKTIME=30
        MAXTRAN=100

*DM_REMOTE
"LOWER"    TYPE=TDOMAIN
        ACCESSPOINTID="LOWER"

*DM_TDOMAIN
"UPPER"  NWADDR="//localhost:11000"
        CONNECTION_POLICY=ON_STARTUP
        RETRY_INTERVAL=60
        MAXRETRY=MAXLONG
        TCPKEEPALIVE=YES

"LOWER"    NWADDR="//localhost:12000"
        CONNECTION_POLICY=ON_STARTUP
        RETRY_INTERVAL=60
        MAXRETRY=MAXLONG
        TCPKEEPALIVE=YES

*DM_EXPORT
TOUPPER
        LACCESSPOINT="UPPER"
        RNAME="TOUPPER"

*DM_IMPORT
TOLOWER
        RACCESSPOINT=LOWER
        LACCESSPOINT=UPPER
        RNAME="TOLOWER"

