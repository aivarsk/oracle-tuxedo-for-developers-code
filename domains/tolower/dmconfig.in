*DM_LOCAL
"LOWER"  GWGRP=DMGROUP
        TYPE=TDOMAIN
        ACCESSPOINTID="LOWER"
        DMTLOGDEV="@CURDIR@/dmtlog"
        DMTLOGNAME="DMTLOG"
        CONNECTION_POLICY=ON_STARTUP
        MAXRETRY=MAXLONG
        RETRY_INTERVAL=60
        BLOCKTIME=30
        MAXTRAN=100

*DM_REMOTE
"UPPER"    TYPE=TDOMAIN
        ACCESSPOINTID="UPPER"

*DM_TDOMAIN
"UPPER"    NWADDR="//localhost:11000"
        CONNECTION_POLICY=ON_STARTUP
        RETRY_INTERVAL=60
        MAXRETRY=MAXLONG
        TCPKEEPALIVE=YES

"LOWER"  NWADDR="//localhost:12000"
        CONNECTION_POLICY=ON_STARTUP
        RETRY_INTERVAL=60
        MAXRETRY=MAXLONG
        TCPKEEPALIVE=YES

*DM_EXPORT
TOLOWER
        LACCESSPOINT="LOWER"
        RNAME="TOLOWER"

*DM_IMPORT
TOUPPER 
  	RACCESSPOINT=UPPER
        LACCESSPOINT=LOWER
        RNAME="TOUPPER"
